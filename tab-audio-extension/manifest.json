{
  "manifest_version": 3,
  "name": "Tab Audio Test",
  "version": "1.0",
  "description": "Captures tab audio",
  "permissions": [
    "tabCapture",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}